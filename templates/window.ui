<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<template class="OcsmWindow" parent="GtkApplicationWindow">
		<property name="title">OCSM</property>
		<child>
			<object class="GtkBox" id="mainBox">
				<property name="orientation">0</property>
				<property name="spacing">10</property>
				
				<child>
					<object class="LeftNav" id="leftNav" />
				</child>
				
				<child>
					<object class="GtkSeparator">
						<property name="orientation">1</property>
					</object>
				</child>
				
				<child>
					<object class="GtkBox">
						<property name="hexpand">1</property>
						<property name="orientation">1</property>
						
						<child>
							<object class="GtkButton" id="button">
							<property name="label">Press me!</property>
							<property name="margin-top">12</property>
							<property name="margin-bottom">12</property>
							<property name="margin-start">12</property>
							<property name="margin-end">12</property>  
							</object>
						</child>
						
						<child>
							<object class="StatefulTrack" id="dots">
								<property name="halign">3</property>
								<property name="max">12</property>
							</object>
						</child>
						
						<child>
							<object class="StatefulButton" id="dot">
								<property name="mode">1</property>
								<property name="halign">3</property>
							</object>
						</child>
						
						<child>
							<object class="StatefulTrack" id="boxes">
								<property name="mode">2</property>
								<property name="halign">3</property>
								<property name="max">10</property>
								<property name="withSpace">0</property>
							</object>
						</child>
						
						<child>
							<object class="StatefulTrack" id="boxes2">
								<property name="mode">3</property>
								<property name="halign">3</property>
								<property name="max">6</property>
								<property name="withSpace">0</property>
							</object>
						</child>
						
						<child>
							<object class="StatefulTrack" id="boxes3">
								<property name="mode">4</property>
								<property name="halign">3</property>
								<property name="max">9</property>
								<property name="withSpace">0</property>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</template>
</interface>
